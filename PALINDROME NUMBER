class Solution {
public:
    bool isPalindrome(int x) {
        
        long long int no=0,y,z=x;
        
        while(x>0)
        {
            y=x%10;
            x/=10;
            no=no*10+y;
        }
            cout<<no<<" ";
        
        
        if(no==z)
            return true;
        
        return false;
    }
};
